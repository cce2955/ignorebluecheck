{
  "manifest_version": 3,
  "name": "Whitelist Manager",
  "version": "1.0",
  "description": "My extension description",
  "host_permissions": [
    "https://twitter.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["content.js"],
      "run_at":"document_start"
    }
  ],  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ]
  
}
