{
  "manifest_version": 3,
  "name": "Whitelist Manager",
  "version": "1.0",
  "description": "My extension description",
  "host_permissions": [
    "https://twitter.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ]
  
}
