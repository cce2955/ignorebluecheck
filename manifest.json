{
    "name": "Verified Account Remover",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Removes tweets that have a verified account badge",
    "content_scripts": [
      {
        "matches": [
          "https://twitter.com/*",
          "http://twitter.com/*",
          "https://www.twitter.com/*",
          "http://www.twitter.com/*"
        ],
        "js": ["content.js"]
      }
    ]
  }
  